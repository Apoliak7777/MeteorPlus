{
  "schemaVersion": 1,
  "id": "meteorplus",
  "version": "${version}",
  "name": "Meteor Plus",
  "description": "Cool add-on for Meteor",
  "authors": [
    "Olejka",
		"Neki_play",
		"Vazgen005"
  ],
  "contact": {
		"issues": "https://github.com/TheSainEyereg/MeteorPlus/issues",
		"sources": "https://github.com/TheSainEyereg/MeteorPlus"
  },

	"license": "GPL-3.0",
  "icon": "assets/plus/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
			"olejka.meteorplus.MeteorPlus"
    ]
  },
  "mixins": [
    "meteor-plus.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "0,140,252",
		"github:sha": "@gh_hash@"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
